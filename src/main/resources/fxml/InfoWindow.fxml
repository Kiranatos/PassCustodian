<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" 
            prefHeight="310.0" 
            prefWidth="560.0" 
            styleClass="mainFxmlClass" 
            xmlns="http://javafx.com/javafx/8.0.111" 
            xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="net.kiranatos.gui.infowindow.InfoWindowController">
    
    <!--
    <stylesheets>
        <URL value="@InfoWindow.css" />
    </stylesheets>
    -->
   
    <children>
      <VBox prefHeight="186.0" 
            prefWidth="560.0" 
            AnchorPane.bottomAnchor="0.0" 
            AnchorPane.leftAnchor="0.0" 
            AnchorPane.rightAnchor="0.0" 
            AnchorPane.topAnchor="0.0">
          
         <children>
            <GridPane>
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="145.0" minWidth="10.0" prefWidth="90.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="376.0" minWidth="10.0" prefWidth="360.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="110.0" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
              </rowConstraints>


              <children>
<!-- BUTTONS FROM GRID -->
                   <Button fx:id="bufferSiteButton"
                           minHeight="25.0" 
                           minWidth="100.0" 
                           mnemonicParsing="false" 
                           onAction="#startListenerInfoWindowButtons" 
                           text="%b1" 
                           textAlignment="CENTER" 
                           GridPane.columnIndex="2">
                     <font>
                        <Font size="12.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </Button>
                  
                  <Button fx:id="bufferLoginButton" 
                          minHeight="25.0" 
                          minWidth="100.0"
                          mnemonicParsing="false" 
                          onAction="#startListenerInfoWindowButtons" 
                          text="%b2" 
                          textAlignment="CENTER" 
                          GridPane.columnIndex="2" 
                          GridPane.rowIndex="1">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Button>
                  
                  <Button fx:id="bufferPassButton" 
                          minHeight="25.0" 
                          minWidth="100.0" 
                          mnemonicParsing="false" 
                          onAction="#startListenerInfoWindowButtons" 
                          text="%b3" textAlignment="CENTER" 
                          GridPane.columnIndex="2" 
                          GridPane.rowIndex="2">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Button>
                  
                  <Button fx:id="bufferMailButton" 
                          minHeight="25.0" 
                          minWidth="100.0" 
                          mnemonicParsing="false" 
                          onAction="#startListenerInfoWindowButtons" 
                          text="%b4" 
                          textAlignment="CENTER" 
                          GridPane.columnIndex="2" 
                          GridPane.rowIndex="3">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Button>
                  
                  <Button fx:id="bufferTagsButton" 
                          layoutX="467.0" layoutY="117.0" minHeight="25.0" minWidth="100.0" 
                          mnemonicParsing="false" onAction="#startListenerInfoWindowButtons" 
                          text="%b5" textAlignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="4">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Button>
                  
                  <Button fx:id="bufferPhoneButton" layoutX="467.0" layoutY="151.0" minHeight="25.0" minWidth="100.0" 
                          mnemonicParsing="false" onAction="#startListenerInfoWindowButtons" 
                          text="%b6" textAlignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="5">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Button>
                  
                  <Button fx:id="bufferNameButton" layoutX="467.0" layoutY="185.0" 
                          minHeight="25.0" minWidth="100.0" mnemonicParsing="false" 
                          onAction="#startListenerInfoWindowButtons" text="%b7" textAlignment="CENTER" 
                          GridPane.columnIndex="2" GridPane.rowIndex="6">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Button>
                  
                  <Button fx:id="bufferInfoButton" layoutX="467.0" layoutY="220.0" 
                          minHeight="25.0" minWidth="100.0" mnemonicParsing="false" 
                          onAction="#startListenerInfoWindowButtons" text="%b8" 
                          textAlignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="7">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Button>
                  
<!-- LABELS FROM GRID -->
                  <Label text="Site: " textAlignment="CENTER">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <font>
                        <Font name="Arial" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Login: " textAlignment="CENTER" GridPane.rowIndex="1">
                     <font>
                        <Font name="Arial" size="14.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <Label text="Password: " textAlignment="CENTER" GridPane.rowIndex="2">
                     <font>
                        <Font name="Arial" size="14.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <Label text="E-Mail: " textAlignment="CENTER" GridPane.rowIndex="3">
                     <font>
                        <Font name="Arial" size="14.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <Label text="Tags:" textAlignment="CENTER" GridPane.rowIndex="4">
                     <font>
                        <Font name="Arial" size="14.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <Label layoutX="10.0" layoutY="150.0" text="Phone:" textAlignment="CENTER" GridPane.rowIndex="5">
                     <font>
                        <Font name="Arial" size="14.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <Label layoutX="10.0" layoutY="185.0" text="Name:" textAlignment="CENTER" GridPane.rowIndex="6">
                     <font>
                        <Font name="Arial" size="14.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <Label layoutX="10.0" layoutY="220.0" text="Information:" textAlignment="CENTER" GridPane.rowIndex="7">
                     <font>
                        <Font name="Arial" size="14.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  
                  
                  
<!-- TextFields FROM GRID -->                  
                  <TextField fx:id="textField_Site"         layoutX="104.0" layoutY="220.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                  <TextField fx:id="textField_Login"        layoutX="104.0" layoutY="220.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="textField_Pass"         layoutX="104.0" layoutY="220.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="textField_Mail"         layoutX="104.0" layoutY="220.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="textField_Tags"         layoutX="104.0" layoutY="220.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <TextField fx:id="textField_Phone"        layoutX="104.0" layoutY="220.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />                  
                  <TextField fx:id="textField_NameSurname"  layoutX="104.0" layoutY="220.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <TextField fx:id="textField_Info"         layoutX="104.0" layoutY="220.0" GridPane.columnIndex="1" GridPane.rowIndex="7" />                  
               </children>
            </GridPane>

                        
<!-- BUTTON "OK" -->            
            <Button fx:id="okInfoWindow" 
                    defaultButton="true" 
                    minHeight="25.0" minWidth="100.0" 
                    mnemonicParsing="false"
                    onAction="#startListenerInfoWindowButtons" 
                    text="OK" 
                    textAlignment="CENTER">
               <font>
                  <Font size="12.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="230.0" right="10.0" />
               </VBox.margin>
            </Button>
            
            
         </children>
         
      </VBox>
   </children>
</AnchorPane>
